{
    "name": "ranyuen/di",
    "description": "Simple Ray.Di style DI (Dependency Injector) extending Pimple.",
    "version": "0.0.2",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Sachirou Inoue",
            "email": "utakata.c4se@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.0",
        "pimple/pimple": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "fabpot/php-cs-fixer": "~1.0@dev",
        "phpmd/phpmd": "~2.1",
        "squizlabs/php_codesniffer": "~1.5"
    },
    "autoload": {
        "psr-4": {"Ranyuen\\": "lib/"}
    },
    "config": {
        "github-protocols": ["https","git","ssh"]
    }
}
